#<%= timelineName %>.flow-artboard {
    width: <%= layer.bounds.size.width %>px;
    height: <%= layer.bounds.size.height %>px;
    overflow: <%= Web.layerMasksToBounds(layer) %>;
    position: relative;
    padding: 0px;
    margin: 0px auto;
    background-color: <%= layer.backgroundColor.hex %>;
    visibility: visible;
    pointer-events: none;
}
