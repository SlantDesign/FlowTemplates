    /**
    * Creates and returns all animations for this timeline.
    */
    createAllAnimations() {
        return [
  <%- if includeSafariArtboardAnimtionBugFix -%>
            this.artboardAnimation(),
  <%- end -%>
    <%- for animationName in animationNames -%>
            this.<%= animationName -%>(),
    <%- end -%>
    <%- for trackName in trackNames -%>
            this.<%= trackName -%>(),
    <%- end -%>
        ].flat()
    }