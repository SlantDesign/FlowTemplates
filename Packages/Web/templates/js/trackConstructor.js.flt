
<%-
// Add the track name to the list of track names to create
var animationName = "\(id)_\(propertyName.snakeCased())"
animationNames = animationNames.appending(animationName) 
-%>
<%= animationName %>() {
    let property = "<%= propertyName -%>";
    let values = <%= values %>;
    let timingFunctions = <%= timingFunctions %>;
    let times = <%= times %>;
    let element = this.rootElement.querySelector(<%= querrySelector %>)

    let <%= id %>_track = new Track(
      property,
      values,
      timingFunctions,
      times,
      element
    );

    return <%= id %>_track;
}