<%-
  var gradient = layer.fillGradient
  var gradientID = "\(id)-fill" 
  var tag = Web.gradientTag(gradient) -%>
<defs id="<%= id %>-defs">
    <<%= tag %> id="<%= gradientID %>" <%= Web.gradientAttributes(gradient) %>>
    <%- insert("svg/stops.svg.flt", from: Web, indentation: indentation + 2) -%>
    </<%= tag %>>
</defs>